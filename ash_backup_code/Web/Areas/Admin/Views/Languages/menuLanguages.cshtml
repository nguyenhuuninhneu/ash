@using Web
@using Web.Model
@{
    Layout = null;
    var lstLanguages = (List<tbl_Languages>)TempData["lstLanguages"];
    var langCodeDefault = (string)Session["langCodeDefaut"];

}
<script src="~/AppJs/languages.js"></script>
@foreach (var item in lstLanguages)
{
    <li onclick='languages.onchangeLanguges("@item.Code")'>
        <a style="cursor:pointer" href="" title="@item.Name"><img width="30" height="20" src="@item.Icon" class="@(langCodeDefault != null && 
        langCodeDefault == item.Code ? "active" : "")"/></a>
    </li>
}
<style>
    .active{
        border:1px solid #fff;
    }
</style>

