@using Web.Model
@using Web.Model.CustomModel
@{
    ViewBag.Title = "Quy trình xuất bản tin bài";
    var lstAction = (List<int>)HttpContext.Current.Session["Action"];
}
 
<script src="~/AppJs/process.js"></script>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Danh sách chuyển xử lý</h3>
                <div style="top: 8px;position: absolute; right: 40px;" class="pull-right">
                    @if (lstAction != null && lstAction.Contains((int)EnumHelper.Action.Delete))
                    {
                        <button type="button" class="btn btn-danger waves-effect waves-light" onclick="process.onmultidelete() "><i class="fa fa-trash-o"></i> Xóa nhiều</button>
                    }
                </div>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="table-responsive" id="gridDataProcess">

                        </div>
                        <div class="col-md-12" id="paginationholder">
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        process.loadData(1);
    });
</script>