@{
    Layout = null;
}

@using Web.Core
@using Web.Model
@model List<Web.Model.PathWay>

@if (Model.Count > 0)
{ 
    <ul id="pageroad">
        @for (int i = Model.Count -1; i >= 0; i--)
        {
            <li class="@Model[i].Active">
                <a href="@Model[i].Link" title="@Model[i].Name">
                    @Model[i].Name
                </a>
            </li>
        } 
    </ul> 
}