<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!-- Basic Page -->
    <meta charset="UTF-8">
    <!-- Mobile Specific -->
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <!-- Favicon -->
    <link href="/Content/Team/images/favicon.png" rel="shortcut icon" type="image/x-icon">
    <link href="/Content/Team/images/favicon.png" rel="icon" type="image/x-icon">

    <!-- BOXICONS -->
    <link href='../../../unpkg.com/boxicons%402.0.7/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="/Content/Team/css/icons.min.css">

    <!-- Plugin -->

    <link rel="stylesheet" href="/Content/Team/libs/pg-calendar-master/pignose.calendar.css">
    <!-- APP CSS -->
    <link rel="stylesheet" href="/Content/Team/css/bootstrap.min.css">
    <link rel="stylesheet" href="/Content/Team/css/grid.css">
    <link rel="stylesheet" href="/Content/Team/css/style.css">
    <link rel="stylesheet" href="/Content/Team/css/responsive.css">
</head>

<body class="sidebar-expand counter-scroll">

    <!-- MAIN CONTENT -->
    <div class="main" style="padding-left: 0">
        @RenderBody()
        <div class="main-content" style="margin-top: 0">
            <section class="login">
                <div class="row">
                    <div class="col-12">
                        <div class="box">
                            <div class="box-header d-flex justify-content-between">
                                <a href="index-2.html">
                                    <img src="/Content/Team/images/logo.png" alt="">
                                </a>

                                <div class="action-reg">
                                    <h4 class="fs-30">Login</h4>
                                    <a href="new-account.html">Sign in to your account</a>
                                </div>

                            </div>
                            <div class="line"></div>
                            <div class="box-body">
                                <div class="auth-content my-auto">

                                    <form class="mt-5 pt-2" action="https://excellencetheme.com/templates/doshtan/index.html">
                                        <div class="mb-24">
                                            <label class="form-label mb-14">Mail or User Name</label>
                                            <input type="text" class="form-control" id="username" placeholder="Your text">
                                        </div>
                                        <div class="mb-16">
                                            <div class="d-flex align-items-start">
                                                <div class="flex-grow-1">
                                                    <label class="form-label mb-14">Password</label>
                                                </div>
                                                <div class="flex-shrink-0">
                                                    <div class="">
                                                        <a href="#" class="text-muted">Forgot password?</a>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="input-group auth-pass-inputgroup">
                                                <input type="password" class="form-control" placeholder="Enter password" aria-label="Password" aria-describedby="password-addon">
                                                <button class="btn shadow-none ms-0" type="button" id="password-addon"><i class="far fa-eye-slash"></i></button>
                                            </div>
                                        </div>
                                        <div class="row mb-29">
                                            <div class="col">
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" id="remember-check">
                                                    <label class="form-check-label fs-14" for="remember-check">
                                                        Remember me
                                                    </label>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="mb-3">
                                            <button class="btn bg-primary color-white w-100 waves-effect waves-light fs-18 font-w500" type="submit">Sign in</button>
                                        </div>
                                    </form>

                                    <div class="mt-20 pt-2 text-center">
                                        <div class="signin-other-title">
                                            <a href="#" class="text-muted fs-14">Forgot password?</a>
                                        </div>

                                    </div>

                                    <div class="mt-37 text-center">
                                        <p class="text-muted mb-0 fs-14">Don't have an account ? <a href="user-login.html" class="text-primary fw-semibold">  Create Account </a> </p>
                                    </div>

                                    <div class="mt-37 text-center">

                                        <ul class="list-inline mb-0">
                                            <li class="list-inline-item">
                                                <a href="javascript:void()" class="social-list-item text-white">
                                                    <i class='bx bxl-facebook-square'></i>
                                                </a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a href="javascript:void()" class="social-list-item text-white">
                                                    <i class='bx bxl-twitter'></i>
                                                </a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a href="javascript:void()" class="social-list-item  text-white">
                                                    <i class='bx bxl-linkedin-square'></i>
                                                </a>
                                            </li>
                                            <li class="list-inline-item">
                                                <a href="javascript:void()" class="social-list-item  text-white">
                                                    <i class='bx bxl-google-plus'></i>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </div>
    <!-- END MAIN CONTENT -->

    <div class="overlay"></div>

    <!-- SCRIPT -->
    <!-- APEX CHART -->

    <script src="/Content/Team/libs/jquery/jquery.min.js"></script>
    <script src="/Content/Team/libs/jquery/jquery-ui.min.js"></script>
    <script src="/Content/Team/libs/moment/min/moment.min.js"></script>
    <script src="/Content/Team/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/Content/Team/libs/peity/jquery.peity.min.js"></script>
    <script src="/Content/Team/libs/chart.js/Chart.bundle.min.js"></script>
    <script src="/Content/Team/libs/owl.carousel/owl.carousel.min.js"></script>
    <script src="/Content/Team/libs/bootstrap/js/bootstrap.min.js"></script>
    <script src="/Content/Team/js/countto.js"></script>
    <script src="/Content/Team/libs/date-picker/datepicker.js"></script>
    <script src="/Content/Team/libs/simplebar/simplebar.min.js"></script>
    <script src="/Content/Team/libs/password/pass-addon.init.js"></script>


    <!-- APP JS -->
    <script src="/Content/Team/js/main.js"></script>
    <script src="/Content/Team/js/shortcode.js"></script>
    <script src="/Content/Team/js/pages/datepicker.js"></script>

    <script>
    </script>
</body>



</html>
